{
  "languages": {
    "python": {
      "name": "Python",
      "extensions": [".py", ".pyw"],
      "single_line": "#",
      "multi_line": null,
      "notes": "Python uses triple quotes are for strings"
    },
    "javascript": {
      "name": "JavaScript",
      "extensions": [".js", ".mjs", ".cjs"],
      "single_line": "//",
      "multi_line": {
        "start": "/*",
        "end": "*/"
      }
    },
    "java": {
      "name": "Java",
      "extensions": [".java"],
      "single_line": "//",
      "multi_line": {
        "start": "/*",
        "end": "*/"
      }
    },
    "c": {
      "name": "C",
      "extensions": [".c", ".h"],
      "single_line": "//",
      "multi_line": {
        "start": "/*",
        "end": "*/"
      }
    },
    "cpp": {
      "name": "C++",
      "extensions": [".cpp", ".hpp", ".cc", ".cxx", ".hxx"],
      "single_line": "//",
      "multi_line": {
        "start": "/*",
        "end": "*/"
      }
    },
    "ruby": {
      "name": "Ruby",
      "extensions": [".rb"],
      "single_line": "#",
      "multi_line": {
        "start": "=begin",
        "end": "=end"
      },
      "notes": "Multi-line delimiters must be at the beginning of the line"
    },
    "html": {
      "name": "HTML",
      "extensions": [".html", ".htm"],
      "single_line": null,
      "multi_line": {
        "start": "<!--",
        "end": "-->"
      }
    },
    "css": {
      "name": "CSS",
      "extensions": [".css"],
      "single_line": null,
      "multi_line": {
        "start": "/*",
        "end": "*/"
      }
    },
    "rust": {
      "name": "Rust",
      "extensions": [".rs"],
      "single_line": "//",
      "multi_line": {
        "start": "/*",
        "end": "*/"
      }
    },
    "lua": {
      "name": "Lua",
      "extensions": [".lua"],
      "single_line": "--",
      "multi_line": {
        "start": "--[[",
        "end": "--]]"
      }
    }
  }
}
